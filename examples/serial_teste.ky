// Teste comunicação serial 

setup {
    // Inicia a comunicação com o computador a 9600
    serialBegin 9600
    pin 13 is output
    serialPrintln "Kyber iniciado"
}

loop {
    serialPrintln "loop" //testando se vai criar um void loop
    set pin 13 to high
    serialPrintln "LED Aceso"
    delay 500

    set pin 13 to low
    serialPrintln "LED Apagado"
    delay 500
}